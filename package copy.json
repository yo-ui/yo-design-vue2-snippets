{
	"name": "yo-design-vue2-vs",
	"displayName": "YOUI Vue2.x Snippets",
	"description": "Visual Studio Code YOUI Vuex2.x UI框架VSCode代码提示插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:yo-design-vue2-vs.helloWorld",
		"onLanguage:html",
		"onLanguage:javascript",
		"onLanguage:vue",
		"onLanguage:vue-html",
		"onLanguage:jsx"
	],
	"languages": [
		{
			"id": "vue",
			"extensions": [
				".vue",
				".jsx",
				".js",
				".html",
				".htm"
			],
			"aliases": [
				"Vue",
				"VUE",
				"vue"
			]
		}
	],
	"icon": "images/logo.png",
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"title": "Yo-Design-Vue2 Snippets",
			"properties": {
				"yo.showTip": {
					"type": "boolean",
					"default": true,
					"description": "是否在每次启动的时候显示欢迎提示"
				}
			}
		},
		"commands": [
			{
				"command": "yo-design-vue2-vs.helloWorld",
				"title": "Yo-Design-Vue2 Snippets"
			}
		],
		"keybindings": [
			{
				"command": "yo-design-vue2-vs.helloWorld",
				"key": "ctrl+f2",
				"mac": "cmd+f2",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus&&resourceLangId===html",
					"command": "yo-design-vue2-vs.helloWorld",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "yo-design-vue2-vs.helloWorld",
					"group": "navigation"
				}
			]
		},
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/html.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			}
		],
		"viewsWelcome": [],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "beautifulGirl",
					"title": "美女",
					"icon": "images/logo.png"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "nodeDependencies",
					"name": "Node Dependencies",
					"when": "workspaceHasPackageJSON",
					"contextualTitle": "Package Explorer"
				}
			],
			"beautifulGirl": [
				{
					"id": "beautifulGirl1",
					"name": "国内美女"
				},
				{
					"id": "beautifulGirl2",
					"name": "国外美女"
				}
			]
		},
		"iconThemes": [
			{
				"id": "testIconTheme",
				"label": "测试图标主题",
				"path": "./theme/icon-theme.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"bugs": {
		"url": "https://gitee.com/yo-ui/yo-design-vue2-vs/issues",
		"email": "1034850788@qq.com"
	},
	"homepage": "https://gitee.com/yo-ui/yo-design-vue2-vs.git",
	"repository": {
		"type": "git",
		"url": "https://gitee.com/yo-ui/yo-design-vue2-vs.git"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}