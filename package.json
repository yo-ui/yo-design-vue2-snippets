{
	"name": "yo-design-vue2-snippets",
	"displayName": "YOUI Vue2.x Snippets",
	"description": "Visual Studio Code YOUI Vuex2.x UI框架VSCode代码提示插件",
	"version": "0.0.2",
	"publisher": "yo-design-vue2-snippets",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"onCommand:yodv-helper.snippets",
		"onLanguage:javascript",
		"onLanguage:vue",
		"onLanguage:vue-html",
		"onLanguage:html",
		"onLanguage:jsx"
	],
	"icon": "images/logo.png",
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Yo-Design-Vue2 Snippets Configuration",
			"properties": {
				"yo.showTip": {
					"type": "boolean",
					"default": true,
					"description": "是否在每次启动的时候显示欢迎提示"
				},
				"yo.docUrl": {
					"type": "string",
					"default": "https://yo-ui.gitee.io/youi/",
					"description": "YOUI文档说明地址"
				},
				"yo.indent-size": {
					"type": "number",
					"default": 2,
					"description": "代码片段默认缩进空格数"
				},
				"yo.quotes": {
					"type": "string",
					"default": "double",
					"description": "引号默认使用双引号"
				}
			}
		},
		"commands": [
			{
				"command": "yodv-helper.snippets",
				"title": "Yo-Design-Vue2 Snippets"
			}
		],
		"keybindings": [
			{
				"command": "yodv-helper.snippets",
				"key": "ctrl+f2",
				"mac": "cmd+f2",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus&&resourceLangId===html",
					"command": "yodv-helper.snippets",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "yodv-helper.snippets",
					"group": "navigation"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			}
		],
		"viewsWelcome": [],
		"viewsContainers": {
			"activitybar": []
		},
		"views": {
			"explorer": [
				{
					"id": "nodeDependencies",
					"name": "Node Dependencies",
					"contextualTitle": "Package Explorer"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"bugs": {
		"url": "https://gitee.com/yo-ui/yo-design-vue2-snippets/issues",
		"email": "1034850788@qq.com"
	},
	"homepage": "https://yo-ui.gitee.io/youi/",
	"repository": {
		"type": "git",
		"url": "https://gitee.com/yo-ui/yo-design-vue2-snippets.git"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"pretty": "^2.0.0",
		"shelljs": "^0.8.3"
	}
}